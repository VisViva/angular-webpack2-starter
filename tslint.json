{
    "rulesDirectory": "node_modules/tslint-microsoft-contrib",
    "rules": {
        "no-banned-terms": true,
        "no-cookies": false,
        "no-delete-expression": true,
        "no-disable-auto-sanitization": true,
        "no-document-domain": true,
        "no-document-write": true,
        "no-duplicate-key": true,
        "no-eval": false,
        "no-exec-script": true,
        "no-function-constructor-with-string-args": true,
        "no-http-string": [true, "http://www.example.com/?.*", "http://www.examples.com/?.*"],
        "no-inner-html": true,
        "no-octal-literal": true,
        "no-reserved-keywords": false,
        "no-string-based-set-immediate": true,
        "no-string-based-set-interval": true,
        "no-string-based-set-timeout": true,
        "use-strict": true,
        "forin": false,
        "jquery-deferred-must-complete": true,
        "label-position": true,
        "label-undefined": true,
        "mocha-avoid-only": true,
        "mocha-no-side-effect-code": true,
        "no-any": false,
        "no-arg": true,
        "no-backbone-get-set-outside-model": true,
        "no-bitwise": true,
        "no-conditional-assignment": true,
        "no-console": [true, "debug", "info", "log", "time", "timeEnd", "trace"],
        "no-constant-condition": true,
        "no-control-regex": true,
        "no-debugger": true,
        "no-duplicate-case": true,
        "no-duplicate-variable": true,
        "no-empty": true,
        "no-increment-decrement": false,
        "no-invalid-regexp": true,
        "no-invalid-this": true,
        "no-jquery-raw-elements": true,
        "no-regex-spaces": true,
        "no-sparse-arrays": true,
        "no-stateless-class": false,
        "no-string-literal": false,
        "no-unnecessary-bind": true,
        "no-unnecessary-override": true,
        "no-unsafe-finally": true,
        "no-use-before-declare": true,
        "no-with-statement": true,
        "promise-must-complete": true,
        "radix": true,
        "switch-default": true,
        "triple-equals": [true, "allow-null-check"],
        "use-isnan": true,
        "use-named-parameter": true,
        "valid-typeof": true,
        "arrow-parens": false,
        "chai-prefer-contains-to-index-of": true,
        "chai-vague-errors": true,
        "class-name": true,
        "comment-format": true,
        "export-name": false,
        "function-name": true,
        "import-name": true,
        "interface-name": true,
        "jsdoc-format": true,
        "max-file-line-count": true,
        "max-func-body-length": [true, 200, {
            "ignore-parameters-to-function-regex": "describe"
        }],
        "max-line-length": false,
        "member-access": true,
        "member-ordering": true,
        "mocha-unneeded-done": true,
        "new-parens": true,
        "no-construct": true,
        "no-constructor-vars": false,
        "no-default-export": true,
        "no-empty-interfaces": true,
        "no-for-in": false,
        "no-function-expression": true,
        "no-inferrable-types": false,
        "no-multiline-string": true,
        "no-null-keyword": false,
        "no-relative-imports": false,
        "no-require-imports": false,
        "no-shadowed-variable": true,
        "no-typeof-undefined": false,
        "no-unnecessary-field-initialization": true,
        "no-unnecessary-local-variable": true,
        "no-unsupported-browser-code": true,
        "no-var-keyword": true,
        "no-var-requires": false,
        "no-var-self": true,
        "object-literal-sort-keys": false,
        "one-variable-per-declaration": true,
        "only-arrow-functions": false,
        "ordered-imports": false,
        "prefer-array-literal": false,
        "prefer-const": true,
        "typedef": [true, "callSignature", "indexSignature", "parameter", "propertySignature", "variableDeclarator", "memberVariableDeclarator"],
        "underscore-consistent-invocation": true,
        "variable-name": false,
        "align": false,
        "curly": true,
        "eofline": true,
        "no-consecutive-blank-lines": true,
        "no-empty-line-after-opening-brace": true,
        "no-single-line-block-comment": true,
        "no-trailing-whitespace": true,
        "no-unnecessary-semicolons": true,
        "object-literal-key-quotes": [true, "as-needed"],
        "one-line": [true, "check-open-brace", "check-catch", "check-else", "check-whitespace"],
        "quotemark": [true, "single"],
        "semicolon": [true, "always"],
        "trailing-comma": [true, {
            "singleline": "never",
            "multiline": "never"
        }],
        "typedef-whitespace": false,
        "whitespace": [true, "check-branch", "check-decl", "check-operator", "check-separator", "check-type"]
    }
}
